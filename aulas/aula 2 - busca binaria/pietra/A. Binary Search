#include <bits/stdc++.h>
#define f first
#define s second
#define _ ios_base::sync_with_stdio(0);cin.tie(0);
#define endl '\n'
#define dbg(x) cout << #x << " = " << x << endl
typedef long long ll;
const int INF = 0x3f3f3f3f;
const ll LINF = 0x3f3f3f3f3f3f3f3fll;
using namespace std;

int solve() {
	int n, q; cin >> n >> q;

	vector<int> a(n);
	for (int i = 0; i < n; i++) {
		cin >> a[i];
	}

	while (q--) {
		int x; cin >> x;

		int l = 0, r = n-1;
		while (l <= r) {
			int m = l + (r - l) / 2;

			if (x >= a[m]) {
				l = m + 1;
			} else {
				r = m - 1;
			}
		}

		cout << (a[l-1] == x ? "YES" : "NO") << endl;
	}

	return(0);
}

int main()
{
    _;

	int t = 1; //cin >> t;
	while (t--) {
		solve();
	}
    
    return(0);
}
