#include <bits/stdc++.h>
#define f first
#define s second
#define _ ios_base::sync_with_stdio(0);cin.tie(0);
#define endl '\n'
#define dbg(x) cout << #x << " = " << x << endl
typedef long long ll;
const int INF = 0x3f3f3f3f;
const ll LINF = 0x3f3f3f3f3f3f3f3fll;
using namespace std;

void solve() {
	int a, b, l;
	cin >> a >> b >> l;

	int ca, cb;
	ca = cb = 0;

	int la = l;
	while (la % a) {
		la /= a;	
		ca++;
	}
	int lb = l;
	while (lb % b) {
		lb /= b;	
		cb++;
	}


}

int main()
{
    _;

	int t; cin >> t;
	while (t--) {
		solve();
	}
    
    return(0);
}
